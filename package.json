{
  "name": "@tvh-val/react-ui",
  "version": "1.0.0",
  "repository": "https://github.com/ValnetineKul/-tvh-val-react-ui.git",
  "scripts": {
    "postci:build": "sh scripts/post-build.sh",
    "postversion": "cp package.json ..",
    "ci:install": "yarn --frozen-lockfile --ignore-optional --ignore-engines --non-interactive --prefer-offline",
    "ci:lint": "eslint --max-warnings 0 ./src && prettier --check ./src",
    "ci:test": "jest --ci --silent --workerIdleMemoryLimit=1GB --maxWorkers=2",
    "ci:sonar": "yarn sonar-scanner -Dsonar.host.url=https://sonarqube.tools.parts.tvh.com -Dsonar.login=\"${SONAR_LOGIN}\" -Dsonar.pullrequest.key=\"${BUILD_TAG}\" -Dsonar.pullrequest.branch=\"${BRANCH_NAME}\" -Dsonar.pullrequest.base=master",
    "ci:build": "tsc -p tsconfig.build.json",
    "ci:publish": "HUSKY=0 semantic-release",
    "ci:storybook": "build-storybook --quiet && cp lighttpd.conf storybook-static/ && cp health.html storybook-static/",
    "lint": "eslint --fix --cache --max-warnings 0 ./src && prettier --ignore-unknown --write --cache ./src",
    "test": "jest --reporters=default --workerIdleMemoryLimit=1GB --maxWorkers=4",
    "build": "tsc -p tsconfig.build.json",
    "prebuild": "mkdir -p dist",
    "postbuild": "sh scripts/post-build.sh",
    "watch": "tsc --watch -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start": "yarn storybook",
    "prepare": "husky install",
    "update-icons": "ts-node ./src/themes/core/iconsTransporter/index.ts",
    "update-tokens": "ts-node src/themes/core/tokensParser/index.ts"
  },
  "main": "./index.d.ts",
  "types": "./index.d.ts",
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@hookform/resolvers": "2.0.0",
    "@mui/lab": "5.0.0-alpha.60",
    "@mui/material": "5.2.4",
    "@mui/styles": "^5.2.1",
    "@svgr/webpack": "^6.5.1",
    "@tvh/react-common": "1.4.4",
    "classnames": "^2.2.6",
    "country-iso-3-to-2": "^1.1.0",
    "date-fns": "^2.29.3",
    "deepmerge": "^4.2.2",
    "figma-api": "^1.10.1",
    "locale-currency": "^0.0.2",
    "notistack": "2.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.18.0",
    "react-input-mask": "^2.0.4",
    "react-number-format": "^4.7.3",
    "react-phone-number-input": "^3.1.41",
    "react-router-dom": "6.2.2",
    "react-scroll": "^1.8.2",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "swiper": "6.8.4",
    "throttle-debounce": "^3.0.1",
    "tss-react": "^3.2.4",
    "uuid": "^8.3.2",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^9.5.0",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/client-api": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.0",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.1.9",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^29.2.4",
    "@types/node": "^14.14.33",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-scroll": "^1.8.2",
